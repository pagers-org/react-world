{
  "editor.tabSize": 2,
  "search.exclude": {
    "**/.yarn": true,
    "**/.pnp.*": true,
    "**/*.map": true
  },
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "eslint.nodePath": ".yarn/sdks",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "[typescript]": {
    "editor.formatOnSave": true
  }
}
